/* pages/product/product.wxss */
#container{
  background: #F9F9F9;
}
.tableView{
  width: 100%;
  display: flex;
  flex-flow: row wrap;
  justify-content: center;
  align-items: center;
}
.tag{
  width: 40%;
  height: 550rpx;
  flex: 1 0 auto;
  margin: 0 1% 2% 1%;
  text-align: center;
  background: #FFFFFF;
}
.tag view{
  width: 90%;
  height: 75%;
  margin: 0 auto;
}
.tag image{
  width: 260rpx;
  height: 260rpx;
  border-radius: 55rpx;
  display: block;
  padding: 20rpx;
  margin: 0 auto;
}
.tag text{
  margin-top: 1%;
  color: #707070;
  font-size: 26rpx;
  display: block;
}